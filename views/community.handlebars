  <div class="row">
  <h1>Community Bulletin Board</h1>
    <div class="col s12" id="bulletinBoard">
      {{#each messages }}
        <div id="messageBoardPost">
          <h5 class="messageBoardHeading">{{ this.event }}</h5>
          <p class="messageBoardDescription">{{ this.description }}</p>
          <p class="messageBoardDate">{{ this.date }}</p>
          <p class="messageBoardName">Posted By: {{this.username}}</p>
        </div>
      {{/each}}
    </div>
  </div>

  <div class="row">
    <div class="col s6">
        <h4>Post Event</h4>
        <form id="eventForm" action="/event" method="post">
            <label class="col s3 offset3">Name of Event:</label>
            <input type="text" name="event"/><br/>
            <label>Date of Event:</label>
            <input type="date" name="date"/>
            <label>Description:</label>
            <input type="text" name="description"/>
            <input type="submit" value="Submit"/>
        </form>
    </div>
    <div class="col s6">
    </div>
	</div>


